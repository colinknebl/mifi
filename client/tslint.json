{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "jsx-no-lambda": false,
    "jsdoc-format": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false
  }
}
