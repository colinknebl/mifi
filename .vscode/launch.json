{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "API",
      "type": "node",
      "request": "launch",
      "port": 1234,
      "protocol": "inspector",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "start"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Server",
      "type": "node",
      "request": "launch",
      "port": 5683,
      "protocol": "inspector",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "start"
      ],
      "console": "integratedTerminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Chrome",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}/client/src"
    },
    {
      "name": "Launch localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/client/src"
    },
    {
      "name": "Launch index.html",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/client/build/index.html"
    },
  ],
  "compounds": [
    {
      "name": "Full-stack",
      "configurations":["Server", "Launch localhost"]
    }
  ]
}